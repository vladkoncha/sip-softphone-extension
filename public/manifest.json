{
  "manifest_version": 3,
  "name": "SIP Softphone",
  "short_name": "Softphone",
  "version": "1.0",
  "description": "",
  "icons": {
    "192": "/icons/icon192.png"
  },
  "permissions": [],
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["inject.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "/icons/icon192.png"
  }
}
